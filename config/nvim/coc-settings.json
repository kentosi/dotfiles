{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    },
    "kotlin": {
      "command": "/Users/gavnewalkar/code/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
      "filetypes": ["kotlin"],
      "trace.server": "verbose"
    }
  },
  "explorer.width": 30,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings": {
    "<cr>": ["expandable?", "expand", "open"],
    "v": "open:vsplit"
  } 
}
